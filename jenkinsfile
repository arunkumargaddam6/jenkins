#creating a pipelines

pipeline{
    agent any
        stages{
            stage('git checkout'){
                steps{
                    sudo apt-get update
                    sudo apt install python3-pip
                    sudo apt install git
                    pip3 install ansible
                    git clone https://github.com/arunkumargaddam6/jenkins.git
                    cd jenkins
                    ansible-playbook test.yml
                }
            }
        }
}